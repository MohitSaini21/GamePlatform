    <!-- Custom Scipts -->
    <script>
      // Function to fetch users from the /users API
      async function fetchUsers() {
        try {
          // Fetch data from the /users API
          const response = await fetch("/users");

          // Check if the response is OK (status code 200-299)
          if (!response.ok) {
            throw new Error("Failed to fetch users");
          }

          // Parse the JSON response
          const users = await response.json();
          console.log(users);
          displayUsers(users);
        } catch (error) {
          console.error("Error fetching users:", error);
        }
      }
      // Call fetchUsers when the page loads
      window.onload = fetchUsers;

      // Function to display users in the right block
      function displayUsers(users) {
        // Get the element where users will be displayed
        const rightBlock = document.getElementById("right-block");

        // Loop through the users and generate HTML for each user
        users.forEach((user) => {
          console.log(user.userName);

          // Create a new div element to represent the user
          let userDiv = document.createElement("div");
          userDiv.classList.add("server", "online"); // Add necessary classes

          // Create the inner HTML for each user
          userDiv.innerHTML = `
    <div>
      <h6 class="mb-1 d-flex text-center">${user.userName}</h6>
      <br>
      <div class="server_players">TimeSpent: ${getRandomNumber()} Hours</div>
    </div>
  `;

          // Append the userDiv (now a node) to the rightBlock
          rightBlock.prepend(userDiv);
        });
      }
      function getRandomNumber() {
        return Math.floor(Math.random() * 10) + 1;
      }
    </script>
